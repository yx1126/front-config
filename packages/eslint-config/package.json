{
    "name": "@yx1126/eslint-config",
    "version": "1.0.0",
    "type": "module",
    "description": "yx1126's Eslint config",
    "keywords": [
        "lint",
        "eslint",
        "eslint-config"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "author": "yx1126",
    "homepage": "https://github.com/yx1126/front-config#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/yx1126/front-config.git"
    },
    "bugs": {
        "url": "https://github.com/yx1126/front-config/issues"
    },
    "scripts": {
        "build": "tsup --format esm,cjs --clean --dts"
    },
    "devDependencies": {
        "@types/eslint": "^8.56.10",
        "@types/node": "^20.14.9",
        "tsup": "^8.1.0",
        "typescript": "^5.5.2"
    },
    "dependencies": {
        "@typescript-eslint/parser": "^7.14.1",
        "eslint": "^9.5.0",
        "eslint-define-config": "^2.1.0",
        "eslint-plugin-jsonc": "^2.16.0",
        "eslint-plugin-vue": "^9.26.0",
        "eslint-plugin-yml": "^1.14.0",
        "typescript-eslint": "^7.14.1"
    },
    "peerDependencies": {
        "eslint": ">=8.40.0"
    }
}
